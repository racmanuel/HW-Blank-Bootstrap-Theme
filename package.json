{
  "name": "hw-blank-bootstrap-theme",
  "version": "1.0.0",
  "description": "This is a blank Bootstrap 4 starter theme for WordPress. It uses Gulp and allows you to recompile Bootstrap css from Sass. It's focused on minimalism. If you're looking for a full-feature starter theme maybe 'sage' is a better option.",
  "main": "webpack.mix.js",
  "repository": "git@github.com:roccons/HW-Blank-Bootstrap-Theme.git",
  "author": "dev@habitatweb.com.mx",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "yarn development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "yarn development --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "yarn production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "optimize-images": "imagemin assets/img/* --plugin.jpegoptim.progressive=true --plugin.jpegoptim.max=70 --plugin.pngquant --out-dir=public/img"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "2.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "imagemin": "^7.0.1",
    "imagemin-cli": "^6.0.0",
    "imagemin-jpegoptim": "^7.0.0",
    "imagemin-pngquant": "^9.0.0",
    "laravel-mix": "^5.0.4",
    "resolve-url-loader": "3.1.0",
    "sass": "^1.26.9",
    "sass-loader": "8.*",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}
